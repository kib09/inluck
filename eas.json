{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_CLIENT_ID": "GOOGLE_CLIENT_ID",
        "GEMINI_API_KEY": "GEMINI_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_CLIENT_ID": "GOOGLE_CLIENT_ID",
        "GEMINI_API_KEY": "GEMINI_API_KEY"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "GOOGLE_CLIENT_ID": "GOOGLE_CLIENT_ID",
        "GEMINI_API_KEY": "GEMINI_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
